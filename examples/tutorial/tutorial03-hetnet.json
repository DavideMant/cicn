{
    "resources": [
        {
            "type": "Physical",
            "name": "server",
            "hostname": "localhost"
        },
	{
            "type": "Group",
	    "name": "routed"
	},
	{
	    "type": "GUI",
	    "groups": [ "routed" ]
	},
        {
            "type": "LxcImage",
	    "name": "cicn-image",
            "image": "ubuntu1604-cicnsuite-rc3",
            "node": "server"
        },
        {
            "type": "LxcContainer",
            "image": "cicn-image",
            "name": "cons",
            "node": "server",
	    "groups": [ "routed" ],
            "category": "tablet",
            "x": 1,
            "y": 2
        },
        {
            "type": "LxcContainer",
            "image": "cicn-image",
            "name": "wifi",
            "node": "server",
	    "groups": [ "routed" ],
            "category": "wifi",
            "x": 2,
            "y": 1
        },
        {
            "type": "LxcContainer",
            "image": "cicn-image",
            "name": "lte",
            "node": "server",
	    "groups": [ "routed" ],
            "category": "lte",
            "x": 2,
            "y": 3
        },
        {
            "type": "LxcContainer",
            "image": "cicn-image",
            "name": "prod",
            "node": "server",
	    "groups": [ "routed" ],
            "category": "video-server",
            "x": 3,
            "y": 2
        },
        {
            "type": "MetisForwarder",
            "node": "cons"
        },
        {
            "type": "MetisForwarder",
            "node": "wifi"
        },
        {
            "type": "MetisForwarder",
            "node": "lte"
        },
        {
            "type": "MetisForwarder",
            "node": "prod"
        },
        {
            "type": "WebServer",
            "node": "prod",
            "prefixes": [
                "/webserver"
            ]
        },
        {
            "type": "Link",
	    "groups": [ "routed" ],
            "src_node": "wifi",
            "dst_node": "prod"
        },
        {
            "type": "Link",
	    "groups": [ "routed" ],
            "src_node": "lte",
            "dst_node": "prod"
        },
        {
          "type": "EmulatedWiFiChannel",
          "name": "wch",
          "node": "server",
          "ap": "wifi",
          "stations": ["cons"],
	  "groups": [ "routed" ],
          "control_port": 30001
        },
        {
          "type": "EmulatedLteChannel",
          "name": "lch",
          "node": "server",
          "ap": "lte",
	  "groups": [ "routed" ],
          "stations": ["cons"],
          "control_port": 30002
        },
        {
            "type": "CentralIP",
            "ip_routing_strategy": "spt",
	    "groups": [ "routed" ],
	    "ip4_data_prefix": "192.168.42.0/23",
	    "ip6_data_prefix": "9002::/16"
        },
        {
            "type": "CentralICN",
	    "groups": [ "routed" ],
            "face_protocol": "udp4"
        }
    ]
}
